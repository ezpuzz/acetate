<div id="results" class="overflow-auto space-y-2">
    {% for artist in artists|d([]) %}
    <a id="result{{artist.id}}" href="#result{{artist.id}}"
        class="block hover:bg-slate-200 rounded p-2 cursor-pointer target:bg-slate-300" hx-get="/artist/{{artist.id}}"
        hx-target="#details">
        <div class="text-xl">
            {{artist.name}}
        </div>
        <div>{{artist.profile|d()}}</div>
    </a>
    {% endfor %}
</div>